{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-plusplus": "off",
    "no-console": "warn",
    "max-len": ["error", { "comments": 150, "ignoreUrls": true }]
  }
}
